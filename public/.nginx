location / {
    if (!-e $request_filename){
        rewrite  ^(.*)$  /index.php?s=$1  last;   break;
    }
    if ($request_method ~ ^(HEAD)$ ) {
        return 200 "All OK";
    }
}

location ~ ^/(theme|static)/.*\.(html|htm|tpl|md)$ {
    deny all;
}

location ~* \.(css|js)$ {
    if (!-f $request_filename) {
        rewrite ^/.*$ /storage/static.do;
    }
    if ( -f $request_filename ) {
        expires 30d;
    }
}
